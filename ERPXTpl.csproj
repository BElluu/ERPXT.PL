    <RootNamespace>ERPXTpl</RootNamespace>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/BElluu/ERPXT.PL</RepositoryUrl>
